<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ZigzagView">
        <attr name="zigzagHeight" format="dimension" />
        <attr name="zigzagElevation" format="dimension" />
        <attr name="zigzagBackgroundColor" format="color" />
        <attr name="zigzagPaddingContent" format="dimension" />
        <attr name="zigzagPadding" format="dimension" />
        <attr name="zigzagPaddingLeft" format="dimension" />
        <attr name="zigzagPaddingRight" format="dimension" />
        <attr name="zigzagPaddingBottom" format="dimension" />
        <attr name="zigzagPaddingTop" format="dimension" />
        <attr name="zigzagSides">
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
        </attr>
        <attr name="zigzagShadowAlpha" format="float" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="pwText" format="string" />
        <attr name="pwTextColor" format="color" />
        <attr name="pwTextSize" format="dimension" />
        <attr name="pwBarColor" format="color" />
        <attr name="pwRimColor" format="color" />
        <attr name="pwRimWidth" format="dimension" />
        <attr name="pwSpinSpeed" format="dimension" />
        <attr name="pwDelayMillis" format="integer" />
        <attr name="pwCircleColor" format="color" />
        <attr name="pwRadius" format="dimension" />
        <attr name="pwBarWidth" format="dimension" />
        <attr name="pwBarLength" format="dimension" />
        <attr name="pwContourColor" format="color"/>
        <attr name="pwContourSize" format="dimension"/>
    </declare-styleable>


</resources>